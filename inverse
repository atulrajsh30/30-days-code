import java.util.*;

class inverse 
{
    public static void main(String args[])
    {
        Scanner sc= new Scanner (System.in);
        int n= sc.nextInt();
        int s=0;
        int c=1;
        while(n!=0)
        {
            int r=n%10;
            s=s+c*(int)Math.pow(10,(r-1));
            n=n/10;
            c++;
        }
        System.out.println(s);
    }
}
